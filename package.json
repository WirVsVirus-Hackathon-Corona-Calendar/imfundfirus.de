{
  "name": "automatic-gatsbyjs-app-landing-page",
  "description": "Automatically generate iOS app landing page using GatsbyJS",
  "version": "1.0.0",
  "author": "Imed Adel <aadelimed@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.20.6",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.2",
    "gh-pages": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.1.3",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "husky": "^1.3.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.10.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "deploy": "npm run build && gh-pages -d public",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern src/**/*.{js,jsx}"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
